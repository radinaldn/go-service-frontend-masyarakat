<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.topapp.radinaldn.goservicemasyarakat">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_app"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_app"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.LoginActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.MainActivity" />
        <activity android:name=".activity.RegisterActivity" />
        <activity
            android:name=".activity.OrderankuActivity"
            android:label="@string/title_activity_orderanku" />
        <activity
            android:name=".activity.ProfilActivity"
            android:label="@string/title_activity_profil" />
        <activity
            android:name=".activity.SaldoActivity"
            android:label="@string/title_activity_saldo"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>